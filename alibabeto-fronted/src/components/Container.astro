---
interface Props {
    background?: string
    classes?: string
}
const { 
    background = 'transparent',
    classes = ""
} = Astro.props
---

<div
    style={{
        background
    }}
    class={classes}
>
    <section class="max-w-[1100px] container mx-auto px-10 md:px-0"
>
    <slot/>
</section>
</div>

